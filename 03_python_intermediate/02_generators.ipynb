      "A\n"
     ]
    }
   ],
   "source": [
    "# generator function\n",
    "def my_gen():\n",
    "    yield 'A'\n",
    "    yield 'B'\n",
    "    yield 'C'\n",
    "    yield 'D'\n",
    "    \n",
    "# generator object\n",
    "g = my_gen()\n",
    "\n",
    "# print the generator object\n",
    "print(next(g))"
   ]
  },
